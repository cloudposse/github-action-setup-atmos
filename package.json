{
  "name": "github-action-typescript-template",
  "version": "0.0.0",
  "private": true,
  "description": "A template for creating a GitHub Action written in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint --fix src/**/*.ts",
    "lint:check": "eslint src/**/*.ts",
    "build": "ncc build src/setup-atmos.ts -C --license licenses.txt -o dist",
    "build:wrapper": "ncc build src/wrapper.ts -C --license licenses.txt -o dist/wrapper",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelsoncanarinho/typescript-action"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Cloud Posse LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/cache": "^3.0.6",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "axios": "^1.2.1",
    "octokit": "^2.0.10"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "@vercel/ncc": "0.36.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "jest": "29.3.1",
    "jest-mock-axios": "^4.7.0-beta2",
    "jest-nock": "^0.2.2",
    "msw": "^0.49.2",
    "ncc": "^0.3.6",
    "nock": "^13.2.9",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "16.19.0",
    "yarn": "1.22.19"
  }
}

