{
  "name": "github-action-setup-atmos",
  "version": "0.0.0",
  "private": true,
  "description": "A GitHub action to setup the atmos cli",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/setup-atmos.ts -C --license licenses.txt -o dist",
    "build:wrapper": "ncc build src/wrapper.ts -m -C --license licenses.txt -o dist/wrapper",
    "clean": "rimraf dist",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint --fix src/**/*.ts",
    "lint:check": "eslint src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudposse/github-action-setup-atmos"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Cloud Posse LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/cache": "^3.0.6",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "axios": "^1.2.1",
    "octokit": "^2.0.10",
    "replace-in-file": "^6.3.5",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "27.6.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "jest": "29.7.0",
    "jest-mock-axios": "^4.7.3",
    "jest-nock": "^0.2.2",
    "msw": "^0.49.2",
    "ncc": "^0.3.6",
    "nock": "^13.2.9",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "typescript": "5.3.3"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "1.22.19"
  }
}
